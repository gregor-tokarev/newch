rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /tracks/{track} {
      allow update, delete, write: if request.auth != null && resourse.data.creator == request.auth.uid;
      allow read, create: if request.auth != null;
    }

    match /boards/{board} {
        allow read: if true;
    }
  }
}
